{
    "version": 2,
    "builds":[
        {
            "src": "DAAS_API/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLamdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": { "distdir": "staticfiles_buils" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "DAAS_API/wsgi.py"
        }
    ]
}